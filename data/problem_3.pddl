(define (problem blocks_world_five_blocks_problem)
    (:domain blocks_world)
    (:objects blue green1 green2 red1 red2 - block arm - robot_arm table1 - table)
    (:init (clear green1) (clear green2) (empty) (on blue red1) (on green1 blue) (on green2 red2) (ontable red1) (ontable red2))
    (:goal (and (on green2 green1) (on green1 blue) (on blue red2) (on red2 red1) (ontable red1)))
)