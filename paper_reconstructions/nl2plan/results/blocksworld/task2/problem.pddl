(define (problem blocksworld_problem)
    (:domain blocksworld)
    (:objects blue green red yellow - block the_table - location)
    (:init (at red the_table) (at yellow the_table) (clear blue) (clear green) (free) (on blue red) (on green yellow))
    (:goal (and (at red the_table) (on green red) (on blue green) (on yellow blue)))
)